<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.logic.jellyfish.ui.main.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/main_fragment">
        <action
            android:id="@+id/action_mainFragment_to_mapActivity"
            app:destination="@id/mapActivity" />
    </fragment>
    <activity
        android:id="@+id/mapActivity"
        android:name="com.logic.jellyfish.ui.map.MapActivity"
        android:label="MapActivity"
        tools:layout="@layout/map_activity" />

</navigation>